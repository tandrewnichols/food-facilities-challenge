{
  "name": "db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/db/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": ".",
        "outputPath": "dist/libs/db",
        "main": "libs/db/src/index.ts",
        "tsConfig": "libs/db/tsconfig.lib.json",
        "assets": ["libs/db/*.md"]
      }
    },
    "migrations": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "dev",
      "options": {
        "cwd": "libs/db",
        "root": "libs/db",
        "parallel": false
      },
      "configurations": {
        "dev": {
          "commands": [
            "npx drizzle-kit generate:pg",
            "npx tsx src/migrate.ts"
          ]
        }
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "dev",
      "options": {
        "cwd": "libs/db",
        "root": "libs/db",
        "parallel": false
      },
      "configurations": {
        "dev": {
          "commands": [
            "npx tsx src/seed.ts"
          ]
        }
      }
    },
    "initialize": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "dev",
      "options": {
        "cwd": "libs/db",
        "root": "libs/db",
        "parallel": false
      },
      "configurations": {
        "dev": {
          "commands": [
            "npx tsx src/initialize.ts"
          ]
        }
      }
    },
    "copy-migrations": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "dev",
      "options": {
        "cwd": "libs/db",
        "root": "libs/db",
        "parallel": false
      },
      "configurations": {
        "dev": {
          "commands": [
            "npx tsx src/copy-migrations.ts"
          ]
        }
      }
    }
  },
  "tags": []
}
